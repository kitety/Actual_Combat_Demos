    "@umijs/fabric": "^2.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "docz": "^2.3.1",
    "gatsby-plugin-less": "^3.2.4",
    "husky": "^4.2.5",
    "less": "^3.11.3",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "raw-loader": "^4.0.1",
